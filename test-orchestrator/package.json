{
  "name": "test-orchestrator",
  "version": "1.0.0",
  "description": "A test orchestrator built with Bun runtime",
  "type": "module",
  "main": "src/orchestrator.ts",
  "scripts": {
    "start": "bun run src/orchestrator.ts",
    "dev": "bun --watch src/orchestrator.ts",
